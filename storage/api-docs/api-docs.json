{
    "openapi": "3.0.0",
    "info": {
        "title": "Your super  ApplicationAPI",
        "version": "1.0.0"
    },
    "paths": {
        "/api/doctors": {
            "get": {
                "tags": [
                    "Doctors"
                ],
                "summary": "Get all doctors",
                "operationId": "822344ab2329d71809a283c0f8768d42",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    }
                }
            }
        },
        "/profile/{id}": {
            "post": {
                "tags": [
                    "Patient"
                ],
                "summary": "Update patient profile",
                "description": "Update the profile of a specific patient",
                "operationId": "updateProfilePatient",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the patient",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PatientProfileUpdateRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Profile updated successfully",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "404": {
                        "description": "Patient not found"
                    }
                }
            }
        },
        "/api/patient/signup": {
            "post": {
                "tags": [
                    "Patient"
                ],
                "summary": "Patient SignUp",
                "description": "Register a new Patient",
                "operationId": "signup",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "fullName",
                                    "password",
                                    "phone",
                                    "address"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "fullName": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "phone": {
                                        "type": "string"
                                    },
                                    "address": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful Operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SignInRes"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Email already exists",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Email already exists"
                                        },
                                        "error": {
                                            "type": "string",
                                            "example": "email is error"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "SignInRes": {
                "title": "Sign In Response",
                "description": "Response schema for sign in operation",
                "properties": {
                    "userId": {
                        "description": "User ID",
                        "type": "integer",
                        "format": "int64"
                    },
                    "role": {
                        "description": "User Role",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "PatientProfileUpdateRequest": {
                "required": [
                    "name",
                    "email"
                ],
                "properties": {
                    "name": {
                        "description": "The name of the patient",
                        "type": "string",
                        "example": "John Doe"
                    },
                    "email": {
                        "description": "The email address of the patient",
                        "type": "string",
                        "format": "email",
                        "example": "johndoe@example.com"
                    }
                },
                "type": "object"
            }
        }
    }
}